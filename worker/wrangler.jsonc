{
  "$schema": "node_modules/wrangler/config-schema.json",
  "name": "domain-finder-api",
  "main": "src/index.ts",
  "compatibility_date": "2025-03-10",
  "compatibility_flags": ["nodejs_compat"],

  // Migration to delete old Durable Object
  "migrations": [
    {
      "tag": "v1",
      "new_sqlite_classes": ["DomainFinderMCP"]
    },
    {
      "tag": "v2",
      "deleted_classes": ["DomainFinderMCP"]
    }
  ],

  // Enable observability
  "observability": {
    "enabled": true
  }
}
